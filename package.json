{
  "name": "speech-music-bot",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "npm run build && node lib/index.js",
    "preinstall": "npm run setup:ubuntu",
    "build": "tsc",
    "clean": "npm run service:uninstall && rm -rf lib",
    "production": "npm run clean && npm install --no-save && npm run build && npm run service:install",
    "service:install": "./service.sh install",
    "service:uninstall": "./service.sh uninstall",
    "service:start": "./service.sh start",
    "service:stop": "./service.sh stop",
    "service:restart": "npm run build && ./service.sh restart",
    "service:log": "./service.sh log",
    "setup:ubuntu": "apt-get install -y build-essential python autoconf libtool libatlas-base-dev liblapack-dev libblas-dev ffmpeg",
    "setup:fedora": "dnf install -y make automake gcc gcc-c++ kernel-devel python autoconf libtool atlas-devel openblas ffmpeg sox && ln -s /usr/lib64/atlas/libsatlas.so /usr/lib/libcblas.so && ln -s /usr/lib64/atlas/libsatlas.so.3 /usr/lib/libcblas.so.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^10.14.9",
    "@types/node-cleanup": "^2.1.0",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@google-cloud/speech": "^2.3.1",
    "@types/fluent-ffmpeg": "^2.1.10",
    "@types/request-promise-native": "^1.0.16",
    "@types/ws": "^6.0.1",
    "bufferutil": "^4.0.1",
    "discord.js": "github:discordjs/discord.js#master",
    "erlpack": "github:hammerandchisel/erlpack",
    "fluent-ffmpeg": "^2.1.2",
    "node-cleanup": "^2.1.2",
    "node-opus": "^0.3.2",
    "node-spotify-api": "^1.1.1",
    "prism-media": "github:amishshah/prism-media#master",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "snowboy": "^1.3.1",
    "sodium": "^3.0.2",
    "spotify-web-api-node": "^4.0.0",
    "youtube-dl": "^2.0.0",
    "ytdl-core": "^0.29.2"
  }
}
